package ua.lpnuai.oop.slavchanyk10;

public class Room {
    private String type;
    private int number;

    public Room (String type, int number){
        this.type = type;
        this.number = number;
    }

    public void setNumber(int number) {
        this.number = number;
    }

    public void setType(String type) {
        this.type = type;
    }

    @Override
    public String toString() {
        return "Room{" +
                "type=" + this.type +
                ", number='" + this.number + '\'' +
                '}';
    }
}
